alias venv-create-3.8='python3.8 -m venv ./venv3.8'
alias venv-activate-3.6='source ./venv3.6/bin/activate'
alias venv-activate-3.7='source ./venv3.7/bin/activate'
alias venv-activate-3.8='source ./venv3.8/bin/activate'
