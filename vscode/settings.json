{
  "css.lint.important": "warning",
  "css.validate": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoSurround": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "SF Mono, Menlo, monospace",
  "editor.fontSize": 12,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.hover.sticky": false,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 128,
  "editor.minimap.renderCharacters": true,
  "editor.multiCursorModifier": "alt",
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": false,
  "editor.rulers": [
    80,
    96,
    128
  ],
  "editor.scrollBeyondLastColumn": 4,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "*.static": {
        "fontStyle": "bold",
        "foreground": "#ff0000"
      },
      "type": {
        "foreground": "#00aa00"
      }
    }
  },
  "editor.suggestOnTriggerCharacters": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.pyi": "python",
    "pylintrc": "ini"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.svn": true,
    "**/__pycache__": true,
    "**/_build/debug": true,
    "**/_build/release": true,
    "**/mypy_cache": true
  },
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "html.autoClosingTags": false,
  "html.completion.attributeDefaultValue": "singlequotes",
  "html.format.wrapLineLength": 0,
  "javascript.autoClosingTags": false,
  "jupyter.pylanceHandlesNotebooks": false,
  "macros": {
    "callStackDown": [
      "workbench.debug.action.focusCallStackView",
      "list.focusDown",
      "list.select"
    ],
    "callStackUp": [
      "workbench.debug.action.focusCallStackView",
      "list.focusUp",
      "list.select"
    ]
  },
  "python.analysis.stubPath": "typestubs",
  "python.defaultInterpreterPath": "python3",
  "python.experiments.enabled": false,
  "python.languageServer": "Pylance",
  "python.linting.lintOnSave": false,
  "python.linting.pylintEnabled": false,
  "python.sortImports.path": "isort",
  "python.terminal.activateEnvironment": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "settings.cycle": [
    {
      "id": "trimTrailingWhitespace",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "files.trimTrailingWhitespace": true
        },
        {
          "files.trimTrailingWhitespace": false
        }
      ]
    }
  ],
  "settings.cycle.logLevel": "log",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontFamily": "SF Mono, Menlo, monospace",
  "terminal.integrated.lineHeight": 1,
  "typescript.autoClosingTags": false,
  "window.newWindowDimensions": "maximized",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#5c79ef",
    "editorBracketHighlight.foreground2": "#d6df76",
    "editorBracketHighlight.foreground3": "#c172d9",
    "editorBracketHighlight.foreground4": "#4fb1bc",
    "editorBracketHighlight.foreground5": "#60b069",
    "editorBracketHighlight.foreground6": "#abb2c0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#e82124",
    "statusBar.background": "#181818",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.foreground": "#707070",
    "statusBar.noFolderBackground": "#212121",
    "terminal.ansiBlack": "#404040",
    "terminal.ansiBlue": "#4444FF",
    "terminal.ansiBrightBlack": "#404040",
    "terminal.ansiBrightBlue": "#4444FF",
    "terminal.ansiBrightCyan": "#00FFFF",
    "terminal.ansiBrightGreen": "#00FF00",
    "terminal.ansiBrightMagenta": "#FF00FF",
    "terminal.ansiBrightRed": "#FF2222",
    "terminal.ansiBrightWhite": "#C0C0C0",
    "terminal.ansiBrightYellow": "#FFFF00",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiRed": "#FF2222",
    "terminal.ansiWhite": "#C0C0C0",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF"
  },
  "workbench.colorTheme": "gloss-black",
  "workbench.editor.showIcons": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": false
}
