{
  "[plaintext]": {
    "editor.colorDecorators": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnType": false
  },
  "[scad]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "css.format.enable": false,
  "css.format.maxPreserveNewLines": 2,
  "css.format.newlineBetweenRules": false,
  "css.format.newlineBetweenSelectors": false,
  "css.format.spaceAroundSelectorSeparator": true,
  "css.lint.duplicateProperties": "warning",
  "css.lint.emptyRules": "ignore",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoSurround": "never",
  "editor.bracketPairColorization.enabled": true,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "SF Mono, Menlo, monospace",
  "editor.fontSize": 12,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.inlineSuggest.enabled": true,
  "editor.lightbulb.enabled": "onCode",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 128,
  "editor.minimap.renderCharacters": true,
  "editor.mouseWheelScrollSensitivity": 0.5,
  "editor.multiCursorModifier": "alt",
  "editor.parameterHints.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": false,
  "editor.rulers": [
    80,
    96,
    128
  ],
  "editor.scrollBeyondLastColumn": 4,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {}
  },
  "editor.stickyScroll.enabled": true,
  "editor.suggestOnTriggerCharacters": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.m": "objective-c",
    "*.pyi": "python",
    "pylintrc": "ini"
  },
  "files.dialog.defaultPath": "/Users/gwk/work",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.svn": true,
    "**/__pycache__": true,
    "**/_build/debug": true,
    "**/_build/release": true,
    "**/mypy_cache": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.advanced": {},
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "scminput": false,
    "yaml": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "html.autoClosingTags": false,
  "html.completion.attributeDefaultValue": "singlequotes",
  "html.format.enable": false,
  "html.format.wrapLineLength": 0,
  "javascript.autoClosingTags": false,
  "json.format.enable": false,
  "json.format.keepLines": true,
  "python.analysis.diagnosticMode": "workspace",
  "python.createEnvironment.trigger": "off",
  "python.defaultInterpreterPath": "python3",
  "python.experiments.enabled": false,
  "python.terminal.activateEnvironment": false,
  "python.testing.autoTestDiscoverOnSaveEnabled": false,
  "python.testing.promptToConfigure": false,
  "scad-lsp.fmtExePath": "/opt/homebrew/bin/clang-format",
  "scad-lsp.searchPaths": "${workspaceFolder}/scadlib",
  "scss.format.enable": false,
  "search.useIgnoreFiles": false,
  "settings.cycle": [
    {
      "id": "trimTrailingWhitespace",
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "files.trimTrailingWhitespace": true
        },
        {
          "files.trimTrailingWhitespace": false
        }
      ]
    }
  ],
  "settings.cycle.logLevel": "log",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontFamily": "SF Mono, Menlo, monospace",
  "terminal.integrated.lineHeight": 1,
  "terminal.integrated.mouseWheelScrollSensitivity": 0.5,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.tabs.enabled": false,
  "typescript.autoClosingTags": false,
  "window.newWindowDimensions": "maximized",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#5c79ef",
    "editorBracketHighlight.foreground2": "#d6df76",
    "editorBracketHighlight.foreground3": "#c172d9",
    "editorBracketHighlight.foreground4": "#4fb1bc",
    "editorBracketHighlight.foreground5": "#60b069",
    "editorBracketHighlight.foreground6": "#abb2c0",
    "editorBracketHighlight.unexpectedBracket.foreground": "#e82124",
    "statusBar.background": "#181818",
    "statusBar.debuggingBackground": "#263238",
    "statusBar.foreground": "#707070",
    "statusBar.noFolderBackground": "#212121",
    "terminal.ansiBlack": "#404040",
    "terminal.ansiBlue": "#4444FF",
    "terminal.ansiBrightBlack": "#404040",
    "terminal.ansiBrightBlue": "#4444FF",
    "terminal.ansiBrightCyan": "#00FFFF",
    "terminal.ansiBrightGreen": "#00FF00",
    "terminal.ansiBrightMagenta": "#FF00FF",
    "terminal.ansiBrightRed": "#FF2222",
    "terminal.ansiBrightWhite": "#C0C0C0",
    "terminal.ansiBrightYellow": "#FFFF00",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiGreen": "#00FF00",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiRed": "#FF2222",
    "terminal.ansiWhite": "#C0C0C0",
    "terminal.ansiYellow": "#FFFF00",
    "terminal.background": "#000000",
    "terminal.foreground": "#FFFFFF"
  },
  "workbench.colorTheme": "gloss-black",
  "workbench.editor.showIcons": false,
  "workbench.list.mouseWheelScrollSensitivity": 0.5,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "none"
}
